{
  "name": "fhevm-sdk",
  "version": "1.0.0",
  "description": "Universal framework-agnostic SDK for FHEVM - Build confidential dApps with ease",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "require": "./dist/react.js",
      "import": "./dist/react.esm.js",
      "types": "./dist/react.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:core && npm run build:react",
    "build:core": "node build.js",
    "build:react": "node build-react.js",
    "test": "echo \"SDK tests coming soon\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "privacy",
    "zama",
    "sdk",
    "framework-agnostic"
  ],
  "author": "FHEVM Community",
  "license": "MIT",
  "peerDependencies": {
    "ethers": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "fhevmjs": "^0.5.0"
  },
  "devDependencies": {
    "ethers": "^6.9.0"
  }
}
