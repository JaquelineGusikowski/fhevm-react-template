{
  "name": "fhevm-react-template",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK - Framework-agnostic SDK for building confidential frontend applications with Fully Homomorphic Encryption",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "npm run build:sdk",
    "build:sdk": "npm run build --workspace=packages/fhevm-sdk",
    "dev:nextjs": "npm run dev --workspace=examples/nextjs",
    "dev:farmer": "npm run dev --workspace=examples/farmer-subsidy",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:sdk": "npm run test --workspace=packages/fhevm-sdk",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "rm -rf node_modules packages/*/node_modules examples/*/node_modules packages/*/dist"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "privacy",
    "zama",
    "sdk",
    "react",
    "nextjs",
    "vue",
    "framework-agnostic"
  ],
  "author": "FHEVM Community",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "hardhat": "^2.19.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "dependencies": {
    "@fhevm/solidity": "latest",
    "ethers": "^6.9.0"
  }
}
